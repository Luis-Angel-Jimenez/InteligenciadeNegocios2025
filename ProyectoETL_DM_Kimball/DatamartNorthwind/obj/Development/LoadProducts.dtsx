	ON pr.CategoryID = ca.CategoryID
	INNER JOIN 
	(
	SELECT SupplierID, CompanyName 
	FROM NORTHWND.dbo.Suppliers
	) AS sp
	ON pr.SupplierID =sp.SupplierID</property>
