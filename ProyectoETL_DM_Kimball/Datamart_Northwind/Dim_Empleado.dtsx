	[Stage_Northwind].[dbo].[Stage_Empleado] AS S
ON [Datamart_Northwind].[dbo].[Dim_Employee].employee_id = S.Empleado_Codigo
WHERE [Datamart_Northwind].[dbo].[Dim_Employee].employeeid_nk = ?</property>
